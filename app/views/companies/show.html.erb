<h1>Companies#show</h1>
<p>Find me in app/views/companies/show.html.erb</p>

<table>
    <tr>
        <th>Name</th>
        <th>Subdomain</th>
        <th>Employees</th>
        <th>Conglomerate</th>
    </tr>

    <tr>
        <td><%= @company.name %></td>
        <td><%= @company.name%></td>
        <td><%= @company.subdomain%></td>
        <td><%= @company.employees_number%></td>
        <td><%= @company.conglomerate.name%></td>
    </tr>
</table>

<iframe width="300" height="300" frameborder="0" style="border:0"
 src="https://www.google.com/maps/embed/v1/place?key=<%=Rails.application.credentials.google_maps_api_key%>&q=<%=@company.location%>"
 allowfullscreen>
</iframe>